<div id='tab_file'>

        <div class='col-sm-12'>  

            <div class='form-group'>
                <label for='file'>File</label>
                <input type='text' id='file' name='file' class='form-control' readonly='readonly' value='{{file}}'/>
            </div>

            <div class='form-group'>
                <label>Text Contents</label>
                <div class='float--right full-width'>
                    <pre id='ocr-text'>{{ocr-text}}</pre>
                    <br/>
                </div>
            </div>

        </div>

</div>
